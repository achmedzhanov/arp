<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
  </configSections>
  <appSettings>
  </appSettings>
  <log4net >
    <appender name="asda" type="log4net.Appender.AnsiColorTerminalAppender" >
      <target value="T"/>
      <threshold  value="sdfsdf" />
      <layout type="log4net.Layout.ExceptionLayout" >
      </layout>
    </appender>
    <appender name="1" type="log4net.Appender.RollingFileAppender" >
      <appendToFile value=""/>
      <countDirection value=""/>
      <rollingStyle value="Size" />
      <layout type="log4net.Layout.PatternLayout">
      </layout>
    </appender>
    <appender name="SQL" type="log4net.Appender.RollingFileAppender">
    </appender>
    <appender name="SqlTrace" type="log4net.Appender.RollingFileAppender"> 
      <file value="logs/sql-trace.log"  />
      <appendToFile value="true" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="1048576" />
      <rollingStyle value="Size" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    <root >
      <level value="DEBUG" />
    </root>
    <logger name="log4net.Config.BasicConfigurator" >
      <appender-ref ref="SQL" />
    </logger>
    </log4net>
  
</configuration>
